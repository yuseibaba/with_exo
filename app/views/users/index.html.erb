<div  class="user-index-heading">
  <h2>ユーザー<br>一覧</h2>
</div>

<%= form_with url: users_path, method: :get, local: true, class: 'search-container' do |form| %>
  <div class="resarch">
    <%= form.text_field :search, class:'resarch-form' %>
    <%= form.submit '検索', name: nil, class:'resarch-btn' %>
  </div>
<% end %>

<!--user一覧を表示-->
<%= render 'users', users: @users %>