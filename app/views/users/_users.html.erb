<% if users.any? %>
 <ul class="user-index">
   <% users.each do |user| %>
    <li class="user">
      <img src='/about/about2.png'>
      <div class="user-right">
        <div class="user-name">
          <%= user.name %>
        </div>
        <div class="user-introduce">
          <%= user.introduce_comment %>
        </div>
      </div>
    </li>
   <% end %>
 </ul>
 <%= paginate users %>
<% end %>